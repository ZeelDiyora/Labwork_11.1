#include<stdio.h>

int main(){
    
   int arr[5] = {2,4,1,3,7};
   int i = 0;
    
   printf("Array elements are :\n");
   for(i=0;i<5;i++)
   printf("%d\n",arr[i]);
    
   printf("\n");
    
   printf("Square of each elements are :\n");
   for(i=0;i<5;i++)
    
   printf("%d\n",arr[i] * arr[i]);
   printf("\n");
    
   return 0;
}
